<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane prefHeight="367.0" prefWidth="423.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.csit228f2_2.DisplaySchedController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TableView fx:id="scheduleTable" layoutX="50" layoutY="50">
        <columns>
            <TableColumn text="Course Name">
                <cellValueFactory>
                    <PropertyValueFactory property="courseName"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Class Time">
                <cellValueFactory>
                    <PropertyValueFactory property="classTime"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Location">
                <cellValueFactory>
                    <PropertyValueFactory property="location"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Instructor">
                <cellValueFactory>
                    <PropertyValueFactory property="instructor"/>
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>

<!--    <Button text="Add Class Schedule" onAction="#addClassSchedule" />-->
</AnchorPane>
